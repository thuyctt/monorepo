{
  "github": {
    "enabled": true
  },
  "regions": ["hnd1"],
  "builds": [
    { "src": "home/package.json", "use": "@vercel/next" },
    { "src": "blog/package.json", "use": "@vercel/next" },
    { "src": "design/package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "blog/(.*)", "dest": "blog$1" },
    { "src": "(.*)", "dest": "home$1" }
  ],
  "scope": "Nguyen Thanh Thuy",
  "version": 2
}
